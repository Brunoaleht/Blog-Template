            <template />
            


            <script>
              
import axios from "axios";
import { baseApiUrl, userKey } from "./config/fileGlobal";
import { mapState } from "vuex";
import Header from "./components/templates/Header.vue";
import Footer from "./components/templates/Footer.vue";
import MenuM from "./components/templates/MenuM.vue";
import Content from "./components/templates/Content.vue";
import Loading from "./components/templates/Loading.vue";
export default {
  name: "App",
  components: { Header, Footer, MenuM, Content, Loading },
  computed: mapState(["isMenuVisible", "user"]),
  data() {
    return {
      validatingToken: true,
    };
  },
  methods: {
    async validateToken() {
      this.validatingToken = true;
      const json = localStorage.getItem(userKey);
      const userData = JSON.parse(json);
      this.$store.commit("setUser", null);
      if (!userData) {
        this.validatingToken = false;
        return this.$router.push({ name: "AuthV" });
      }
      const res = await axios.post(\`\${baseApiUrl}/validateToken\`, userData);
      if (res.data) {
        this.$store.commit("setUser", userData);
        if (this.$mq === "xs" || this.$mq === "sm") {
          this.$store.commit("toggleMenu", false);
        }
      } else {
        localStorage.removeItem(userKey);
        this.$router.push({ name: "AuthV" });
      }
      this.validatingToken = false;
    },
  },
  created() {
    this.validateToken();
  },
};

            </script>
            


